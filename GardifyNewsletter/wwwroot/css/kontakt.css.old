select {

    /* styling */
    width: 100%;
    background-color: transparent;
    border: solid 1px #f5f5f5;
    border-radius: 0px;
    display: inline-block;
    font: inherit;
    line-height: 1.5em;
    padding: 0.5em 3.5em 0.5em 1em;

    /* reset */

    margin: 0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-appearance: none;
    -moz-appearance: none;
}


/* arrows */



.col-wow::before {
    font-family: "Font Awesome 5 Free";
    font-weight: 900;
    color: #fff;

    content: "\f078";
    font-size: 25px;
    position: absolute;
    top: 0;
    left: 160px;
    z-index: -1;
    /*   background-color: #d68a27;
    width: 40px;
    height: 40px;*/

}

.col-wow::after {


    content: "";
    font-size: 25px;
    position: absolute;
    top: 0;
    left: 150px;
    z-index: -2;
    background-color: #d68a27;
    width: 40px;
    height: 40px;

}

.col-wot::before {
    font-family: "Font Awesome 5 Free";
    font-weight: 900;
    color: #fff;

    content: "\f078";
    font-size: 25px;
    position: absolute;
    top: 0;
    left: 292px;
    z-index: -1;
    /*
    background-color: #d68a27;
    width: 40px;
    height: 40px;
*/

}

.col-wot::after {


    content: "";
    font-size: 25px;
    position: absolute;
    top: 0;
    left: 284px;
    z-index: -2;
    background-color: #d68a27;
    width: 40px;
    height: 40px;

}





