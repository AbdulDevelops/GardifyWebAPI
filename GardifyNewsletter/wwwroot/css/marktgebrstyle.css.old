body {}

@font-face {
    font-family: 'DINPro-Regular_13937';
    src: url('DINPro-Regular_13937.ttf') format('truetype');
}

@font-face {
    font-family: 'DINPro-Medium_13936';
    src: url('DINPro-Medium_13936.ttf') format('truetype');
}

@font-face {
    font-family: 'DINPro-Light_13935';
    src: url('DINPro-Light_13935.ttf') format('truetype');
}

@font-face {
    font-family: 'DINPro-Bold_13934';
    src: url('DINPro-Bold_13934.ttf') format('truetype');
}

@font-face {
    font-family: 'DINPro-Black_13933';
    src: url('DINPro-Black_13933.ttf') format('truetype');
}

.font-family-regular {
    font-family: 'DINPro-Regular_13937';
}

.font-family-medium {
    font-family: 'DINPro-Medium_13936';

}

.font-family-light {
    font-family: 'DINPro-Light_13935';
}

.font-family-bold {
    font-family: 'DINPro-Bold_13934';
}

.font-family-black {
    font-family: 'DINPro-Black_13933';
}



.letter-spacing {
    letter-spacing: 0.01em;
}

.line_height_36 {
    line-height: 34px;

}

.font-15 {
    font-size: 15px;
}

.font-14 {
    font-size: 14px;
}

.font-16 {
    font-size: 16px;
}

.font-20 {
    font-size: 20px;
}

.font-22 {
    font-size: 20px;
}

.font-25 {

    font-size: 24px;
    /*    font-size: 1.2vw;*/
}

.font-28 {
    font-size: 28px;
}

.font-34 {
    font-size: 34px;
}

.color-gray {
    color: #787977;
}

.color-gd {
    color: #464746;
}

.orangecolor {
    color: #d68a27;
}

.bgorange {
    background-color: #d68a27;
}

.h-gray {
    color: #f5f5f5;
}

.b-gray {
    background-color: #f5f5f5;
}

.form-controlA {
    width: 200px !important;
    height: 35px !important;
    background-color: #f5f5f5;
    border: none;
    border-radius: 0;
}

.btn-controlA {
    width: 78px !important;
    height: 35px;
    background-color: #f5f5f5;
    border: none;
    border-radius: 0;
}

.btn-control {
    width: 78px !important;
    height: 35px;
    background-color: #f5f5f5;
    border: none;
    border-radius: 0;
}

.r-nav{
    height: 4.5rem;
}

#navbarNav li a:hover {
    color: #787977;
    border-bottom: solid 3px #d68a27;

}

.col-content {
    border-left: solid 2px #f6f6f6;
}

.btn-abs {
    height: 3rem;
    border-radius: 0px !important;
    border: 0px;

}

.btn-abs:hover {
    background-color: #787977 !important;

}



.btn-content {
    width: 100%;
    height: 3rem;
    border-radius: 0px !important;
    border: 0px;

}

.btn-content:hover {
    background-color: #787977 !important;

}

.pd {
    border: 0px;
}











@media (max-width: 1141px) {


    .font-25 {
        font-size: 19px !important;

    }

    .form-be {
        margin-top: 0px;
        width: 100%;

    }

    .font-28 {
        font-size: 27px;
    }

    .btn-content {
        height: 3rem;


    }

    .btn-abs {
        height: 3.6rem;


    }

}

@media (max-width: 769px) {

    .font-22 {
        font-size: 18px !important;
    }

    .form-controlA {
        width: 100px !important;
        height: 35px !important;


    }

    .btn-controlA {
        width: 54px !important;

    }

    .nav-to {
        border: 0px !important;

    }

    .font-25 {
        font-size: 19px !important;

    }

    .form-be {
        margin-top: 26px;
        height: 180px !important;
    }

    .font-28 {
        font-size: 26px;
    }

    .btn-content {
        height: 3.2rem;


    }


}


@media (max-width: 576px) {







    .col-nav {
        height: 7vh !important;
        z-index: 11;
    }


    .form-control {
        width: 170px !important;
        height: 35px !important;
        background-color: #f5f5f5;
        border: none;
        border-radius: 0;
    }

    .btn-control {
        width: 20vw !important;

    }

    .font-22 {
        font-size: 22px !important;
    }





    .nav-to {
        border: 0px !important;

    }

    .font-28 {
        font-size: 22px;
    }

    .bord {
        visibility: hidden;
    }

    .dummy {
        visibility: hidden;
    }
}
